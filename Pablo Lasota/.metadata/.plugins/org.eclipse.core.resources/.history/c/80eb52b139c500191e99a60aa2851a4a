#include <stdio.h>
#include <stdlib.h>

int pedirNum(int,int,int);

int main(void)
{

	int aux;
	int *max;
	int *min;
}

int pedirNum(int num,int *max,int *min)
{
	do
	{
		char verificar;
		int aux;
		printf("Ingrese un numero:");
		scanf("%d\n",&num);
		aux = num;
		if(aux < num)
		{
			min = aux;
		}
		if(aux > num)
		{
			max = aux;
		}
		printf("Â¿Desea continuar?s/n\n");
		scanf("%s",&verificar);

	}while(verificar == 's');
}
